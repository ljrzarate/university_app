<h1>New Teacher</h1>
<div class="row">
	<div class="span6 offset3">
		<%= form_for(@teacher) do |f| %>

		<%= f.label :name %>
		<%= f.text_field :name %>

		<%= f.label :identification %>
		<%= f.text_field :identification %>

		<%= f.label :profession %>
		<%= f.text_field :profession %>

		<%= f.label :email %>
		<%= f.text_field :email %>

		<%= f.label :addres %>
		<%= f.text_field :addres%>

		<%= f.label :cellphone %>
		<%= f.text_field :cellphone%>

		<%= f.label :area_teacher %>
		<%= f.text_field :area_teacher%>

		<%= f.submit "Create Teacher", class: "btn btn-large btn-primary" %>
		<% end %>
	</div>
</div>
